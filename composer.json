{
    "name": "rui_mashita/laravel-dblib-odbc-sqlserver",
    "type": "library",
    "description": "PDO DBLIB or PDO ODBC SQL Server Driver for Laravel 4",
    "keywords": ["odbc","laravel"],
    "authors": [
        {
            "name": "Takuya Wakisaka",
            "email": "takuya.wakisaka@moldweorp.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.6.*",
        "illuminate/database": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "DblibOdbcSqlServer\\Database\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
     "laravel": {
         "providers": [
             "DblibOdbcSqlServer\\Database\\DBILIBServiceProvider"
         ]
     }
   }
}
